@article{retsDjankuatGlacierStation2019,
  title = {Djankuat Glacier Station in the {{North Caucasus}}, {{Russia}}: A Database of Glaciological, Hydrological, and Meteorological Observations and Stable Isotope Sampling Results during 2007\textendash 2017},
  shorttitle = {Djankuat Glacier Station in the {{North Caucasus}}, {{Russia}}},
  author = {Rets, Ekaterina P. and Popovnin, Viktor V. and Toropov, Pavel A. and Smirnov, Andrew M. and Tokarev, Igor V. and Chizhova, Julia N. and Budantseva, Nadine A. and Vasil'chuk, Yurij K. and Kireeva, Maria B. and Ekaykin, Alexey A. and Veres, Arina N. and Aleynikov, Alexander A. and Frolova, Natalia L. and Tsyplenkov, Anatoly S. and Poliukhov, Aleksei A. and Chalov, Sergey R. and Aleshina, Maria A. and Kornilova, Ekaterina D.},
